{
  "short_name": "TabbyMons",
  "name": "TabbyMons",
  "description": "Gotta tab 'em all",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://unpkg.com/pokeapi-js-wrapper/dist/index.js"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ]
}